<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- 进度条为0%，使用的图片效果 -->
    <item android:id="@android:id/background" android:drawable="@drawable/security_progress_bg"></item>
    <!-- 进度条为0%-100%，使用的图片效果 -->
    <item android:id="@android:id/secondaryProgress" android:drawable="@drawable/security_progress"></item>
    <!-- 进度条为100%，使用的图片效果 -->
    <item android:id="@android:id/progress" android:drawable="@drawable/security_progress"></item>

</layer-list>
